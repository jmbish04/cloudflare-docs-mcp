    "@cloudflare/agents": "0.0.16",
    "hono": "^4.4.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240512.0",
    "@types/node": "^24.8.1",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "wrangler": "^4.43.0"
  }
}
